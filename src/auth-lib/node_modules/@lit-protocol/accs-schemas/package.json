{
  "name": "@lit-protocol/accs-schemas",
  "version": "0.0.6",
  "license": "MIT",
  "types": "./types/index.d.ts",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    },
    "./schemas/*": {
      "import": "./esm/schemas/*.json",
      "require": "./cjs/schemas/*.json"
    }
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "build:classes": "tsx ./src/gen-interfaces.ts",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:ts": "npm run build:cjs && npm run build:esm",
    "copy-package": "cp ./package.json ./dist/package.json",
    "build": "yarn clean && yarn build:ts && yarn copy-package",
    "prettier:check": "prettier --check \"./src/**/*.ts\"",
    "prettier": "prettier --write \"./src/**/*.ts\"",
    "test": "mocha"
  },
  "dependencies": {
    "ajv": "^8.12.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.6",
    "chai": "^5.0.0",
    "json-schema-to-typescript": "^13.1.2",
    "mocha": "^10.2.0",
    "prettier": "^3.2.5",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
